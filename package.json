{
  "name": "PJ_TEST",
  "version": "1.0.0",
  "description": "IBENIFIT TEST",
  "main": "index.js",
  "author": "Vu Duy Anh",
  "license": "MIT",
  "scripts": {
    "start": "node ./src/app/server.js",
    "dev": "nodemon ./src/app/server.js",
    "eject": "nodejs-scripts eject",
    "eslint": "eslint --ext js,ts,tsx",
    "lint": "eslint './src/**/*.{js,jsx}'",
    "lint:fix": "eslint './src/**/*.{js,jsx}' --fix",
    "lint:css": "stylelint src/**/*.css",
    "prettify": "prettier --write"
  },
  "dependencies": {
    "bcryptjs": "2.4.3",
    "cors": "2.8.5",
    "dotenv": "16.0.0",
    "express": "4.18.1",
    "express-graphql": "0.12.0",
    "graphql": "16.4.0",
    "jsonwebtoken": "8.5.1",
    "mysql2": "2.3.3",
    "nodemon": "2.0.16",
    "sequelize": "6.19.0"
  },
  "devDependencies": {
    "sequelize-cli": "6.4.1"
  },
  "eslintConfig": {
    "extends": "nodejs"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "npm": ">=6.4.1",
    "node": ">=10.13.0"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "npm run eslint -- --fix"
    ],
    "*.{md,json}": [
      "prettier --write"
    ]
  },
  "husky(remove-everything-in-these-parentheses.See-the-issue-#29)": {
    "hooks": {
      "pre-commit": "npm run checkTs && lint-staged"
    }
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*/*.d.ts",
      "!src/**/*/Loadable.{js,jsx,ts,tsx}",
      "!src/**/*/types.ts",
      "!src/index.tsx",
      "!src/serviceWorker.ts"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 90,
        "lines": 90,
        "statements": 90
      }
    }
  }
}